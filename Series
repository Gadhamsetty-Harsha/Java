import java.util.*;
public class Series
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int i=1,b;
        for(i=1;i<=a;i++)
        {
            if(i%2==1)
            {
                b=(i*i)+2;
        System.out.printf(b + " ");
            }
            else
            {
                b=(i*i)-2;
        System.out.printf(b + " ");
            }
        }
    }
}
