import java.util.Scanner;
public class Series8
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a=4;
        System.out.print(4);
        double j;
        for(int i=1;i<=n-1;i++)
        {
            j=a+Math.pow(i,2);
            System.out.printf("% d",(int)j);
            a=(int)j;
        }
    }
}
