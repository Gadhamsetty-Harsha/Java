import java.util.*;
public class Factorial
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=1,c=1;
        while(b<a)
        {
            c=c+1;
            b=b*c;
        }
        if(b==a)
        {
            System.out.print("yes");
        }
        else
        {
            System.out.print("no");
        }
    }
}
