import java.util.*;
public class Avg
{
    public static void main(String args[])
    {
      Scanner Sc=new Scanner(System.in);
      float a,b,c,d,e,f;
      a=Sc.nextFloat();
      b=Sc.nextFloat();
      c=Sc.nextFloat();
      d=Sc.nextFloat();
      e=Sc.nextFloat();
      f=(a+b+c+d+e)/5;
      System.out.printf("%.2f",f);
     
    }
}
