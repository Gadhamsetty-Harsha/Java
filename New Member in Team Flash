import java.util.*;
public class Flash
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        double x,y;
        int x1=sc.nextInt();
        int y1=sc.nextInt();
        int x2=sc.nextInt();
        int y2=sc.nextInt();
        int x3=sc.nextInt();
        int y3=sc.nextInt();
        x=(x1+x2+x3);
        y=(y1+y2+y3);
        System.out.printf("%.1f\n%.1f",x/3,y/3);
    }
}
