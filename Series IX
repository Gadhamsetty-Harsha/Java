import java.util.Scanner;
public class Series9
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        long n=sc.nextInt();
        long a=2;
        long x;
        for( int i=0;i<n;i++)
        {
           x=(a*a)-1;
           System.out.printf("% d",a);
           a=x;
        }
    }
}
