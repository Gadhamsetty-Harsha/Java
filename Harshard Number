import java.util.*;
public class Harshard
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int sum=0,b;
        b=a;
        while(a>0)
        {
            int rem=a%10;
            a=a/10;
            sum=sum+rem;
        }
        if (b%sum==0)
        {
           System.out.print("Harshad Number");
        }
        else
        {
            System.out.print("Not Harshad Number");
        }
    }
}
