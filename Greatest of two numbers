import java.util.*;
public class Compare
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a,b;
        a=sc.nextInt();
        b=sc.nextInt();
        if(a==b)
        {
           System.out.print(+a+ " is equal to " +b);
        }
        else if(a>b)
        {
            System.out.print(+a+ " is greater than "+b);
        }
        else
        {
            System.out.print(+a+ " is less than "+b);
        }
    }
}
